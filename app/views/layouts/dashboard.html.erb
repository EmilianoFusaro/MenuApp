        <script src="/js/icheck.min.js" type="text/javascript"></script>                           <!--RIPRISTINARE-->
        <!-- Touch x Mobile -->
        <script src="/js/jquery.ui.touch-punch.min.js"></script>                                   <!--RIPRISTINARE-->
        <!-- AdminLTE App -->
        <!-- <script src="js/AdminLTE/app.js" type="text/javascript"></script> -->
        <script src="/js/AdminLTE/applicazione.js" type="text/javascript"></script>                <!--RIPRISTINARE-->
