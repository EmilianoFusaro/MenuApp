    <body class="skin-blue">
        <!-- header logo: style can be found in header.less -->
        <header class="header">
            <a href="index.html" class="logo">
                <!-- Add the class icon to your logo image or logo icon to add the margining -->
                MenuApp
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>

                <div class="navbar-right">
                    <ul class="nav navbar-nav">

                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="glyphicon glyphicon-user"></i>
                                <span><%= current_user.name %><i class="caret"></i></span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header bg-light-blue">
                                  <% if current_user.profile.logo != nil %>
                                    <img src="<%= current_user.profile.logo %>" class="img-circle" alt="User Image" id="logo_testa_1"/>
                                  <% else %>
                                    <img src="/images/logo2.png" class="img-circle" alt="User Image" id="logo_testa_1"/>
                                  <% end %>
                                    <p>
                                        <%= current_user.name %>
                                    </p>
                                </li>
                                <!-- Menu Body -->
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <!-- <a href="#" class="btn btn-default btn-flat">Profile</a> -->
                                    </div>
                                    <div class="pull-right">
                                        <!-- <a href="#" class="btn btn-default btn-flat">Esci</a> -->
                                        <%= link_to "Esci", destroy_user_session_path , method: :delete ,class:"btn btn-default btn-flat" %>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>

            </nav>
        </header>
        <div class="wrapper row-offcanvas row-offcanvas-left">
            <!-- Left side column. contains the logo and sidebar -->
            <aside class="left-side sidebar-offcanvas">
                <!-- sidebar: style can be found in sidebar.less -->
                <section class="sidebar">
                    <!-- Sidebar user panel -->
                    <div class="user-panel">
                        <div class="pull-left image">
                            <!-- <img src="/img/avatar3.png" class="img-circle" alt="User Image" /> -->
                            <% if current_user.profile.logo != nil %>
                              <img src="<%= current_user.profile.logo %>" class="img-circle" alt="User Image" id="logo_testa_2"/>
                            <% else %>
                              <img src="/images/logo2.png" class="img-circle" alt="User Image" id="logo_testa_2"/>
                            <% end %>
                        </div>
                        <div class="pull-left info">
                            <p>Ciao <%= current_user.name %></p>
                            <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                        </div>
                    </div>

                    <!-- sidebar menu: : style can be found in sidebar.less -->
                    <ul class="sidebar-menu">
                      <!-- <li class="active"> -->
                        <li class="menu index">
                          <%= link_to dashboard_index_path ,:class=>"menu" do %>
                          <i class="fa fa-dashboard"></i> <span>Pannello Di Controllo</span>
                          <% end %>
                        </li>

                        <li class="menu azienda">
                          <%= link_to dashboard_azienda_path ,:class=>"menu" do %>
                          <i class="fa fa-home"></i> <span>Azienda</span>
                          <% end %>
                        </li>

                        <li class="menu media">
                          <%= link_to dashboard_media_path ,:class=>"menu" do %>
                          <i class="fa fa-camera"></i> <span>Libreria Media</span>
                          <% end %>
                        </li>

                        <li>
                            <%= link_to dashboard_dishes_path ,:class=>"menu" do %>
                                <i class="fa fa-th"></i> <span>Piatti</span>
                            <% end %>
                        </li>

                        <li class="treeview">
                            <a href="#">
                                <i class="fa fa-bar-chart-o"></i>
                                <span>Memu xx</span>
                                <i class="fa fa-angle-left pull-right"></i>
                            </a>
                            <ul class="treeview-menu">
                                <li><a href="pages/charts/morris.html"><i class="fa fa-angle-double-right"></i> Morris</a></li>
                                <li><a href="pages/charts/flot.html"><i class="fa fa-angle-double-right"></i> Flot</a></li>
                                <li><a href="pages/charts/inline.html"><i class="fa fa-angle-double-right"></i> Inline charts</a></li>
                            </ul>
                        </li>

                    </ul>
                </section>
                <!-- /.sidebar -->
            </aside>

            <!-- Right side column. Contains the navbar and content of the page -->

            <aside class="right-side">
                <!-- Content Header (Page header) -->
                <section class="content-header">
                    <h1>
                        <%= yield(:v_titolo) %>
                        <small><%= yield(:v_minides) %></small>
                    </h1>
                    <ol class="breadcrumb">
                        <li><a href="#"><i class="fa fa-dashboard"></i><%= yield(:v_minides) %></a></li>
                        <li class="active"><%= yield(:v_titolo) %></li>
                    </ol>
                </section>

                <!-- Main content -->
                <section class="content">

                <!-- Main row -->
                <%= yield %>

                </section>
            </aside>
        </div>

        <!-- add new calendar event modal -->


        <!-- jQuery 2.0.2 -->
        <%= javascript_include_tag 'gestione_prodotti', media: 'all' %>
        <!-- <script src="/js/jquery.min.js"></script> -->
        <script src="/js/jquery-ui-1.10.3.min.js" type="text/javascript"></script>          <!--RIPRISTINARE-->
        <!-- <script src="/js/jquery.jscroll.min.js" type="text/javascript"></script> -->   <!--JSCROLL-->
