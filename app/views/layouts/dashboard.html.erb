        <script src="/js/bootbox.min.js"></script>  <!--Serve Per Chiedere Msgbox di Conferma-->   <!--RIPRISTINARE-->
        <!-- importa layout bootstrap per le select -->
        <script src="/js/bootstrap-select.min.js"></script> <!--RIPRISTINARE-->


        <script>
